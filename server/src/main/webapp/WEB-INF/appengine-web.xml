<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>oneditx</application>
  <version>1</version>
  <sessions-enabled>false</sessions-enabled>
  <threadsafe>true</threadsafe>
  <system-properties>
    <property name="in.gae.j" value="true" />
    <property name="scalate.allowReload" value="false" />
    <property name="scalate.workdir" value="WEB-INF" />
  </system-properties>
  <resource-files>
    <include path="/templates/**.jade" />
  </resource-files>
  <static-files>
    <include path="/public/**.js" expiration="7d" />
    <include path="/public/**.css" expiration="7d" />
  </static-files>
</appengine-web-app>
